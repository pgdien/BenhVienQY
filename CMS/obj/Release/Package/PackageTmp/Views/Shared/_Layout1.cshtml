    <div class="banner" id="home" style="background-image: url(&quot;http://demo1007.web30s.vn/datafiles/1/sub_banner/14895612836929_1.jpg&quot;)">
        <div class="container">
            <div class="dv-lang">
            </div>
            <div class="logo">
                <h1><a href="@Url.Action("Index","Home")">Trung tâm y tế QY Kính Chào Quý Khách!</a></h1>
            </div>
            <section class="slider container">
                <div class="flexslider">

                    <div class="flex-viewport" style="overflow: hidden; position: relative;">
                        <div id="carousel-example-generic" class="carousel slide" data-ride="carousel" ng-controller="bannerController">
                            <!-- Indicators -->
                            <ol class="carousel-indicators">
                                <li data-target="#carousel-example-generic" data-slide-to="{{index}}" ng-class="{'active':index==0}" ng-repeat="(index, value) in banners"></li>
                            </ol>

                            <!-- Wrapper for slides -->
                            <div class="carousel-inner" role="listbox" style="height:31em;">
                                <div class="item " ng-class="{'active':index==0}" ng-repeat="(index, value) in banners">
                                    <img ng-src="{{value.image}}" class="img-responsive" style="width:100%; height:30em" />
                                    <div class="carousel-caption bannerText">
                                        <a href="{{value.link}}" class="bannerTitle">{{value.title}}</a>
                                        <br /><br />
                                        <p class="bannerContent">{{value.description}}</p>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                </div>
            </section>
        </div>

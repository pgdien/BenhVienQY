    <div class="col-md-12 col-xs-12 col-sm-12 banner">
        <section class="slider container" style="margin:0;width:100%;">
            <div class="flexslider">

                <div class="flex-viewport" style="overflow: hidden; position: relative;">
                    <div id="carousel-example-generic" class="carousel slide" data-ride="carousel" ng-controller="bannerController">
                        <!-- Indicators -->
                        <ol class="carousel-indicators">
                            <li data-target="#carousel-example-generic" data-slide-to="{{index}}" ng-class="{'active':index==0}" ng-repeat="(index, value) in banners"></li>
                        </ol>

                        <!-- Wrapper for slides -->
                        <div class="carousel-inner" role="listbox">
                            <div class="item " ng-class="{'active':index==0}" ng-repeat="(index, value) in banners">
                                <img ng-src="{{value.image}}" class="img-responsive bannerMain" style="width:100%;" />
                                <div class="carousel-caption bannerText">
                                    <a href="{{value.link}}" class="bannerTitle" style="margin-bottom:0.5em;">{{value.title}}</a>
                                    <p class="bannerContent">{{value.description}}</p>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

            </div>
        </section>

    <div class="head_top header_admin">
        <div class="bg_top">
            <script>
                function jsupdate(k, idclass, jscolor) {
                    if (k == 0) $("." + idclass).css("background-color", "#" + jscolor);
                    else if (k == 1) $("." + idclass).css("color", "#" + jscolor);
                }
            </script>
